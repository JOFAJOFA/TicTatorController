<h1 class="text-center">Click Play-button to start!</h1>
<br>
<div class="row">
	<div class="col-lg-offset-2 col-lg-8">
		<div class="messages"></div>
		<div class="turn"></div>
		<table class="table tictac">
			<tr>
				<td id="0" class="square"></td>
				<td id="1" class="square"></td>
				<td id="2" class="square"></td>
			</tr>
			<tr>
				<td id="0" height="140px" class="square"></td>
				<td id="1" height="140px" class="square"></td>
				<td id="2" height="140px" class="square"></td>
			</tr>
			<tr>
				<td id="0" height="140px" class="square"></td>
				<td id="1" height="140px" class="square"></td>
				<td id="2" height="140px" class="square"></td>
			</tr>
		</table>
	</div>
</div>
<div class="row">
	<div class="col-lg-offset-2 col-lg-8 text-center">
		<div class="btn-group btn-group-justified" role="group"
			aria-label="...">
			<div class="btn-group" role="group">
				<button class="btn btn-primary" type="button">Play</button>
			</div>
			<div class="btn-group" role="group">
				<button class="btn btn-danger" type="button">Forfeit</button>
			</div>
		</div>
	</div>
</div>
